// based on a keyboard map from an 'xkb/symbols/de' file

default
xkb_symbols "basic" {

    include "latin(type4)"
    include "kpdl(comma)"
    include "level3(ralt_switch)"

    name[Group1]="German Modded";


    key <BKSL> { [ numbersign,              apostrophe,              rightsinglequotemark,    grave                     ] };
    key <TLDE> { [ asciicircum,             degree,                  NoSymbol,                NoSymbol] };
    key <LSGT> { [ less,                    greater,                 bar,                     U2225                     ] };

    key <AE01> { [ 1,                       exclam,                  NoSymbol,                U263A                     ] };
    key <AE02> { [ 2,                       quotebl,                 NoSymbol,                U1F44D                    ] };
    key <AE03> { [ 3,                       section,                 NoSymbol,                U1F937                    ] };
    key <AE04> { [ 4,                       EuroSign,                NoSymbol,                U1F926                    ] };
    key <AE10> { [ 0,                       equal,                   braceright,              U2248                     ] };
    key <AE11> { [ backslash,               question,                ssharp,                  NoSymbol                  ] };
    key <AE12> { [ grave,                   dead_circumflex,         cedilla,                 NoSymbol                  ] };
    key <AD12> { [ plus,                    asterisk,                asciitilde,              dead_macron               ] };
    key <AB08> { [ comma,                   semicolon,               periodcentered,          multiply                  ] };
    key <AB09> { [ period,                  colon,                   U2026,                   division                  ] };
    key <AB10> { [ minus,                   underscore,              endash,                  U01C0                     ] };

    key <AD01> { [ q,                       Q,                       at,                      U211A                     ] };
    key <AD02> { [ w,                       W,                       Greek_omega,             Greek_OMEGA               ] };
    key <AD03> { [ e,                       E,                       Greek_epsilon,           U2203                     ] };
    key <AD04> { [ r,                       R,                       Greek_rho,               U211D                     ] };
    key <AD05> { [ t,                       T,                       Greek_tau,               partialderivative         ] };
    key <AD06> { [ z,                       Z,                       Greek_zeta,              NoSymbol                  ] };
    key <AD07> { [ u,                       U,                       Greek_eta,               includedin                ] };
    key <AD08> { [ i,                       I,                       Greek_iota,              integral                  ] };
    key <AD09> { [ o,                       O,                       intersection,            elementof                 ] };
    key <AD10> { [ p,                       P,                       Greek_pi,                Greek_PI                  ] };
    key <AD11> { [ braceleft,               braceright,              udiaeresis,              Udiaeresis                ] };
    key <AC01> { [ a,                       A,                       Greek_alpha,             U2200                     ] };
    key <AC02> { [ s,                       S,                       Greek_sigma,             Greek_SIGMA               ] };
    key <AC03> { [ d,                       D,                       Greek_delta,             Greek_DELTA               ] };
    key <AC04> { [ f,                       F,                       Greek_phi,               Greek_PHI                 ] };
    key <AC05> { [ g,                       G,                       Greek_gamma,             Greek_GAMMA               ] };
    key <AC06> { [ h,                       H,                       Greek_psi,               Greek_PSI                 ] };
    key <AC07> { [ j,                       J,                       Greek_theta,             Greek_THETA               ] };
    key <AC08> { [ k,                       K,                       Greek_kappa,             multiply                  ] };
    key <AC09> { [ l,                       L,                       Greek_lambda,            U2112                     ] };
    key <AC10> { [ parenleft,               parenright,              odiaeresis,              Odiaeresis                ] };
    key <AC11> { [ bracketleft,             bracketright,            adiaeresis,              Adiaeresis                ] };
    key <AB01> { [ y,                       Y,                       Greek_upsilon,           nabla                     ] };
    key <AB02> { [ x,                       X,                       Greek_xi,                U00AC                     ] };
    key <AB03> { [ c,                       C,                       Greek_chi,               U2227                     ] };
    key <AB04> { [ v,                       V,                       NoSymbol,                U2228                     ] };
    key <AB05> { [ b,                       B,                       Greek_beta,              U21D0                     ] };
    key <AB06> { [ n,                       N,                       Greek_nu,                U21D2                     ] };
    key <AB07> { [ m,                       M,                       Greek_mu,                ifonlyif                  ] };

};
